<div class="navbar navbar-default navbar-fixed-top">
	<div class="container">
		<div class="navbar-header">
			<a href="../" class="navbar-brand">ET Lists</a> 
		</div>
	</div>
</div>
<div class="container">
<div class="page-header" id="banner">
	<div class="row">
	  <div class="col-lg-6">
	    <h1>New List</h1>
	    <p class="lead">Enter and create a new list</p>
	  </div>
	  <div class="col-lg-6" style="padding: 15px 15px 0 15px;">
	    <div class="well">
      		<%= form_for :list, url: lists_path  do |f| %>
				<p>
			    	<%= f.label 'list name' %><br>
			    	<%= f.text_field :listname %>
		  		</p>
		  		<p>
		    		<%= f.submit %>
		  		</p>
			<% end %>
	    </div>
	  </div>
	</div>
</div>
</div>
